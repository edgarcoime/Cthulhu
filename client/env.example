# Environment variables for Cthulhu Client
# Copy this file to .env.local and update the values as needed

# Application Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_NAME=Cthulhu
NEXT_PUBLIC_VERSION=0.1.0

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
ROOT_DOMAIN=http://localhost:3000

# Optional: Add more environment variables as needed
# NEXT_PUBLIC_FEATURE_FLAG_ENABLED=true
